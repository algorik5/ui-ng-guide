    "dev:ssr": "ng run ui-ng-guide:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run ui-ng-guide:server:production",
    "prerender": "ng run ui-ng-guide:prerender"
  },
